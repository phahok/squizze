{
  "name": "squizze",
  "description": "Quizz generator for Single Page Apps",
  "version": "0.1.0",
  "author": "Jota Teles <josetelesmaciel@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "ava",
    "test-watch": "npm test -- --watch",
    "deploy": "gh-pages-deploy -- '#31 - updates gh-pages.' "
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ],
    "noprompt": true
  },
  "ava": {
    "files": [
      "./src/**/**/*.specs.js"
    ],
    "source": [
      "./src/**/*.{js}",
      "!./src/**/*.specs.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "djv": "2.0.0",
    "vue": "^2.5.16",
    "vue-router": "3.0.1",
    "vuex": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "ava": "0.25.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-stage-3": "6.24.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "file-loader": "1.1.4",
    "gh-pages-deploy": "0.5.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.5.3",
    "sass-loader": "6.0.6",
    "vue-loader": "13.0.5",
    "vue-template-compiler": "2.5.16",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  }
}
